{
    "manifest_version": 3,
    "name": "toriwatch",
    "version": "1.0",
    "description": "Displays the predicted bot percentage of an account in X.",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  